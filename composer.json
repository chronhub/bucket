{
    "name": "chronhub/bucket",
    "type": "project",
    "description": "Token bucket.",
    "keywords": ["tokens", "bucket", "rate", "limit", "throttle", "chronhub"],
    "license": "MIT",
    "require": {
        "php": "^8.2"
    },
    "require-dev": {
        "laravel/pint": "^1.3",
        "pestphp/pest": "2.x-dev",
        "nunomaduro/collision": "^7.1"
    },
    "autoload": {
        "psr-4": {
            "Chronhub\\Bucket\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Chronhub\\Bucket\\Tests\\": "tests/"
        }
    },

    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "php-http/discovery": true
        }
    },
    "minimum-stability": "dev"
}
